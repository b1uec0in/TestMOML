<MOML>
    <DATASOURCE id="coverflowData" src="/www/res/coverflow/coverflowData.xml"/>

    <UILAYOUT portrait="320,460" landscape="320,460">
        <WINDOW layout="0,0,320,460">
            <WEBVIEW2 id="webview" layout="0,0,320,460" src="www/index.html" supportZoom="zoom" clientControl="false"/>
        </WINDOW>
    </UILAYOUT>

    <FUNCTION id="notification_restart">
        <FUNCTIONITEM cmd="{device.systemPopup('Alert', 'You selected button 1', 'OK', '', '')}"/>
    </FUNCTION>

    <FUNCTION id="notification_Exit">
        <FUNCTIONITEM cmd="{device.systemPopup('Alert', 'You selected button 2', 'OK', '', '')}"/>
    </FUNCTION>
</MOML>